#include <bits/stdc++.h>

using namespace std;

int main ()
{
    int n,t,element;
    cin>>n;
    vector <int> v;
    for (int i=0;i<n;i++)
    {
        cin>>element;
        v.push_back(element);
    }
    for (int i=0;i<n;i++)
    {
        if (v[i]>=38)
        {
           t = (v[i]/5)+1;
           if (5*t-v[i]<3)
           {
               cout<<5*t<<endl;
           }
           else
           {
               cout<<v[i]<<endl;
           }
        }
        else cout<<v[i]<<endl;
    }
}
